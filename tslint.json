{
    "extends": ["tslint-react"],
    "rules": {
      // override tslint-react rules here
      "jsx-wrap-multiline": false,
      "max-line-length": [
          true,
          140
      ],
      "quotemark": [
          true, 
          "single",
          "jsx-double"
      ],
      "ordered-imports": true,
      "member-access": false,
      "no-submodule-imports": false,
      "trailing-comma": false,
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false
    }
  }